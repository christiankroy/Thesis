journal = {Nature},
keywords = {Animals,Animals- Newborn,Folder - piRNAs,Humans,Male,Mice,Mice- Inbred C57BL,MolCel2013 References,Multigene Family,Proteins,RNA,Spermatogenesis,Testis,Time Factors},
