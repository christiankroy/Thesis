title = {{Competing RNA secondary structures are required for mutually exclusive splicing of the Dscam exon 6 cluster}},
url = {http://rnajournal.cshlp.org/content/17/2/222.abstract http://rnajournal.cshlp.org/content/17/2/222.full.pdf},
