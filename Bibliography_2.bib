title = {{A piRNA Pathway Primed by Individual Transposons Is Linked to De Novo DNA Methylation in Mice}},
url = {http://www.sciencedirect.com/science/article/pii/S1097276508006199 http://pdn.sciencedirect.com/science?\_ob=MiamiImageURL\&\_cid=272198\&\_user=115184\&\_pii=S1097276508006199\&\_check=y\&\_origin=article\&\_zone=toolbar\&\_coverDate=26-Sep-2008\&view=c\&originContentFamily=serial\&wchp=dGLzVlS-zSkzS\&md5=7e2ef3c01a65cab67bcd200587bb8cea/1-s2.0-S1097276508006199-main.pdf},
