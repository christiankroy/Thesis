title = {{Mili and Miwi target RNA repertoire reveals piRNA biogenesis and function of Miwi in spermiogenesis}},
url = {http://www.nature.com/nsmb/journal/v19/n8/full/nsmb.2347.html http://www.nature.com/nsmb/journal/v19/n8/pdf/nsmb.2347.pdf},
volume = {19},
