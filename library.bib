@article{Gracheva2011,
abstract = {Vampire bats (Desmodus rotundus) are obligate blood feeders that have evolved specialized systems to suit their sanguinary lifestyle. Chief among such adaptations is the ability to detect infrared radiation as a means of locating hotspots on warm-blooded prey. Among vertebrates, only vampire bats, boas, pythons and pit vipers are capable of detecting infrared radiation. In each case, infrared signals are detected by trigeminal nerve fibres that innervate specialized pit organs on the animal's face. Thus, vampire bats and snakes have taken thermosensation to the extreme by developing specialized systems for detecting infrared radiation. As such, these creatures provide a window into the molecular and genetic mechanisms underlying evolutionary tuning of thermoreceptors in a species-specific or cell-type-specific manner. Previously, we have shown that snakes co-opt a non-heat-sensitive channel, vertebrate TRPA1 (transient receptor potential cation channel A1), to produce an infrared detector. Here we show that vampire bats tune a channel that is already heat-sensitive, TRPV1, by lowering its thermal activation threshold to about 30 °C. This is achieved through alternative splicing of TRPV1 transcripts to produce a channel with a truncated carboxy-terminal cytoplasmic domain. These splicing events occur exclusively in trigeminal ganglia, and not in dorsal root ganglia, thereby maintaining a role for TRPV1 as a detector of noxious heat in somatic afferents. This reflects a unique organization of the bat Trpv1 gene that we show to be characteristic of Laurasiatheria mammals (cows, dogs and moles), supporting a close phylogenetic relationship with bats. These findings reveal a novel molecular mechanism for physiological tuning of thermosensory nerve fibres.},
annote = {Warm-blooded sensing isoform},
author = {Gracheva, Elena O and Cordero-Morales, Julio F and Gonz\'{a}lez-Carcac\'{\i}a, Jos\'{e} a and Ingolia, Nicholas T and Manno, Carlo and Aranguren, Carla I and Weissman, Jonathan S and Julius, David},
doi = {10.1038/nature10245},
file = {:Users/royc/Dropbox/Mendeley-Files/2011/Nature/Ganglion-specific splicing of TRPV1 underlies infrared sensation in vampire bats/Gracheva et al/2011 - Nature - Gracheva et al. - Ganglion-specific splicing of TRPV1 underlies infrared sensation in vampire bats.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
keywords = {AS Func,Alternative Splicing,Alternative Splicing: genetics,Amino Acid Sequence,Animals,Cattle,Chiroptera,Chiroptera: anatomy \& histology,Chiroptera: classification,Chiroptera: genetics,Chiroptera: physiology,Face,Face: anatomy \& histology,Face: innervation,Feeding Behavior,Feeding Behavior: physiology,HEK293 Cells,Hot Temperature,Humans,Infrared Rays,Isoforms,Molecular Sequence Data,Organ Specificity,Organ Specificity: genetics,Phylogeny,Predatory Behavior,Predatory Behavior: physiology,Protein Isoforms,Protein Isoforms: chemistry,Protein Isoforms: genetics,Protein Isoforms: metabolism,Protein Structure,Sensation,Sensation: physiology,TRPV Cation Channels,TRPV Cation Channels: chemistry,TRPV Cation Channels: genetics,TRPV Cation Channels: metabolism,Tertiary,Trigeminal Ganglion,Trigeminal Ganglion: metabolism},
