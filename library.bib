pages = {147--158},
title = {{Meiosis and retrotransposon silencing during germ cell development in mice}},
url = {http://www.sciencedirect.com/science/article/pii/S0301468109001224 http://pdn.sciencedirect.com/science?\_ob=MiamiImageURL\&\_cid=277424\&\_user=115184\&\_pii=S0301468109001224\&\_check=y\&\_origin=article\&\_zone=toolbar\&\_coverDate=31-Mar-2010\&view=c\&originContentFamily=serial\&wchp=dGLbVBA-zSkWb\&md5=4e11592ca2326b7da126a49f995ca554/1-s2.0-S0301468109001224-main.pdf},
