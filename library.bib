keywords = {Amino Acid,Amino Acid Sequence,Bacteriophage T4,Bacteriophage T4: enzymology,Binding Sites,Catalysis,Models,Molecular,Molecular Sequence Data,Polynucleotide 5'-Hydroxyl-Kinase,Polynucleotide 5'-Hydroxyl-Kinase: chemistry,Polynucleotide 5'-Hydroxyl-Kinase: metabolism,Protein Structure,Quaternary,Sequence Homology},
month = jul,
number = {14},
pages = {3873--80},
pmid = {12110598},
title = {{Structure and mechanism of T4 polynucleotide kinase: an RNA repair enzyme.}},
