title = {{Polyacrylamide Gel Electrophoresis of RNA}},
url = {http://cshprotocols.cshlp.org/content/2010/6/pdb.prot5444 http://cshprotocols.cshlp.org/content/2010/6/pdb.prot5444.full.pdf},
volume = {2010},
