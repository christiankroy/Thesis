journal = {Nature Protocols},
keywords = {Cell Extracts,Immunoprecipitation,MicroRNAs,Oligonucleotide Array Sequence Analysis,RNA- Messenger,Ribonucleoproteins},
mendeley-tags = {Cell Extracts,Immunoprecipitation,MicroRNAs,Oligonucleotide Array Sequence Analysis,RNA- Messenger,Ribonucleoproteins},
