issn = {0028-0836},
journal = {Nature},
keywords = {Animals,COS Cells,Osmotic Pressure,Phosphatidylinositol Phosphates,Phosphatidylinositol Phosphates: biosynthesis,Phosphatidylinositols,Phosphatidylinositols: metabolism,Saccharomyces cerevisiae,Saccharomyces cerevisiae: metabolism,Schizosaccharomyces,Schizosaccharomyces: metabolism,Signal Transduction,historical,inositol,phosphatidylinositol phosphates,pi35p,yeast},
