journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {1-Phosphatidylinositol 3-Kinase,Alternative Splicing,Animals,Cells- Cultured,Embryo- Mammalian,Embryonic Stem Cells,Female,Gene Expression Profiling,Genes- Lethal,Immunoblotting,Male,Mice,Mice- Inbred C57BL,Mice- Inbred Strains,Mice- Knockout,Phosphorylation,Protein-Serine-Threonine Kinases,RNA Stability,Reverse Transcriptase Polymerase Chain Reaction},
