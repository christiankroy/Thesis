journal = {Proceedings of the National Academy of Sciences},
month = apr,
number = {17},
pages = {7079 --7082},
title = {{Rapid repetitive element-mediated expansion of piRNA clusters in mammalian evolution}},
url = {http://www.pnas.org/content/106/17/7079.abstract http://www.pnas.org/content/106/17/7079.full.pdf},
