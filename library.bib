issn = {1529-2401},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Animals,Axons,Axons: physiology,Cell Adhesion Molecules,Dendrites,Dendrites: physiology,Drosophila,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila Proteins: physiology,Genetic Variation,Genetic Variation: physiology,Genetically Modified,Membrane Proteins,Membrane Proteins: genetics,Membrane Proteins: physiology,Morphogenesis,Morphogenesis: genetics,Neural Cell Adhesion Molecules,Neural Cell Adhesion Molecules: genetics,Neural Cell Adhesion Molecules: physiology,Protein Isoforms,Protein Isoforms: genetics,Protein Isoforms: physiology},
