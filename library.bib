issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
month = sep,
number = {23},
pages = {3013--3020},
pmid = {23024010},
title = {{Site identification in high-throughput RNA-protein interaction data.}},
url = {http://bioinformatics.oxfordjournals.org/content/28/23/3013.short?rss=1 http://www.ncbi.nlm.nih.gov/pubmed/23024010},
volume = {28},
year = {2012}
}
