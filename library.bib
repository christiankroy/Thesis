mendeley-tags = {Actinin,Animals,Caenorhabditis elegans Proteins,Fibronectins,Mice,Molecular Sequence Data,Oligonucleotides,Organ Specificity,RNA Splicing,RNA-Binding Proteins,Zebrafish,Zebrafish Proteins},
