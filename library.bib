@article{Bolcun-Filas2011,
abstract = {The transcriptional regulation of mammalian meiosis is poorly characterized, owing to few genetic and ex vivo models. From a genetic screen, we identify the transcription factor MYBL1 as a male-specific master regulator of several crucial meiotic processes. Spermatocytes bearing a novel separation-of-function allele (Mybl1repro9) had subtle defects in autosome synapsis in pachynema, a high incidence of unsynapsed sex chromosomes, incomplete double-strand break repair on synapsed pachytene chromosomes and a lack of crossing over. MYBL1 protein appears in pachynema, and its mutation caused specific alterations in expression of diverse genes, including some translated postmeiotically. These data, coupled with chromatin immunoprecipitation (ChIP-chip) experiments and bioinformatic analysis of promoters, identified direct targets of MYBL1 regulation. The results reveal that MYBL1 is a master regulator of meiotic genes that are involved in multiple processes in spermatocytes, particularly those required for cell cycle progression through pachynema.},
author = {Bolcun-Filas, Ewelina and Bannister, Laura A. and Barash, Alex and Schimenti, Kerry J. and Hartford, Suzanne A. and Eppig, John J. and Handel, Mary Ann and Shen, Lishuang and Schimenti, John C.},
doi = {10.1242/dev.067645},
file = {:Users/royc/Dropbox/Mendeley-Files/2011/Development/A-MYB (MYBL1) transcription factor is a master regulator of male meiosis/Bolcun-Filas et al/2011 - Development - Bolcun-Filas et al. - A-MYB (MYBL1) transcription factor is a master regulator of male meiosis.pdf:pdf},
journal = {Development},
keywords = {MolCel2013 References},
mendeley-tags = {MolCel2013 References},
number = {15},
pages = {3319 --3330},
title = {{A-MYB (MYBL1) transcription factor is a master regulator of male meiosis}},
url = {http://dev.biologists.org/content/138/15/3319.abstract http://dev.biologists.org/content/138/15/3319.full.pdf},
volume = {138},
year = {2011}
}
@article{Johnson2009,
abstract = {Our understanding of the evolution, formation, and pathological disruption of human brain circuits is impeded by a lack of comprehensive data on the developing brain transcriptome. A whole-genome, exon-level expression analysis of 13 regions from left and right sides of the mid-fetal human brain revealed that 76\% of genes are expressed, and 44\% of these are differentially regulated. These data reveal a large number of specific gene expression and alternative splicing patterns, as well as coexpression networks, associated with distinct regions and neurodevelopmental processes. Of particular relevance to cognitive specializations, we have characterized the transcriptional landscapes of prefrontal cortex and perisylvian speech and language areas, which exhibit a population-level global expression symmetry. We show that differentially expressed genes are more frequently associated with human-specific evolution of putative cis-regulatory elements. These data provide a wealth of biological insights into the complex transcriptional and molecular underpinnings of human brain development and evolution.},
author = {Johnson, Matthew B and Kawasawa, Yuka Imamura and Mason, Christopher E and Krsnik, Zeljka and Coppola, Giovanni and Bogdanovi\'{c}, Darko and Geschwind, Daniel H and Mane, Shrikant M and State, Matthew W and Sestan, Nenad},
doi = {10.1016/j.neuron.2009.03.027},
file = {:Users/royc/Dropbox/Mendeley-Files/2009/Neuron/Functional and evolutionary insights into human brain development through global transcriptome analysis/Johnson et al/2009 - Neuron - Johnson et al. - Functional and evolutionary insights into human brain development through global transcriptome analysis.pdf:pdf},
