title = {{Argonaute Loading Improves the 5′ Precision of Both MicroRNAs and Their miRNA∗ Strands in Flies}},
url = {http://www.sciencedirect.com/science/article/pii/S0960982207024803 http://pdn.sciencedirect.com/science?\_ob=MiamiImageURL\&\_cid=272099\&\_user=115184\&\_pii=S0960982207024803\&\_check=y\&\_origin=article\&\_zone=toolbar\&\_coverDate=22-Jan-2008\&view=c\&originContentFamily=serial\&wchp=dGLbVlS-zSkzk\&md5=9bf8ce7b733ece6c321acb61ddebb0f0/1-s2.0-S0960982207024803-main.pdf},
