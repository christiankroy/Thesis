journal = {Plant \& cell physiology},
keywords = {Amino Acid Sequence,Arabidopsis,Arabidopsis: genetics,Arabidopsis: growth \& development,Flowers,Flowers: genetics,Flowers: growth \& development,Gene Expression Regulation, Developmental,Gene Expression Regulation, Plant,Genes, Plant,Genetic Complementation Test,Molecular Sequence Data,Phylogeny,Plant Proteins,Plant Proteins: genetics,Plants, Genetically Modified,Plants, Genetically Modified: genetics,Plants, Genetically Modified: growth \& development,RNA, Plant,RNA, Plant: genetics,Sequence Alignment,Silene,Silene: genetics,Silene: growth \& development,Transcription Factors,Transcription Factors: genetics},
month = jun,
number = {6},
pages = {1127--41},
pmid = {19406862},
title = {{A SUPERMAN-like gene is exclusively expressed in female flowers of the dioecious plant Silene latifolia.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19406862},
volume = {50},
year = {2009}
}
