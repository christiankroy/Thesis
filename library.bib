month = mar,
number = {3},
pages = {e1002612},
shorttitle = {Thriving under Stress},
title = {{Thriving under Stress: Selective Translation of HIV-1 Structural Protein mRNA during Vpr-Mediated Impairment of eIF4E Translation Activity}},
url = {http://dx.doi.org/10.1371/journal.ppat.1002612 http://www.plospathogens.org/article/fetchObjectAttachment.action?uri=info:doi/10.1371/journal.ppat.1002612\&representation=PDF},
