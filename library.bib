issn = {1754-2189},
journal = {Nature Protocols},
keywords = {Computational Biology,Folder - Transcript.assembly,Genomics},
language = {en},
mendeley-tags = {Computational Biology,Folder - Transcript.assembly,Genomics},
month = mar,
number = {3},
pages = {562--578},
title = {{Differential gene and transcript expression analysis of RNA-seq experiments with TopHat and Cufflinks}},
url = {http://www.nature.com/nprot/journal/v7/n3/full/nprot.2012.016.html http://www.nature.com/nprot/journal/v7/n3/pdf/nprot.2012.016.pdf},
